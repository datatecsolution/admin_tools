	<queryString>
		<![CDATA[SELECT 
    `articulo`.`codigo_articulo` AS `codigo_articulo`,
    `articulo`.`articulo` AS `articulo`,
    `marcas`.`descripcion` AS `marca`,
    `impuesto`.`porcentaje` AS `impuesto`,
    `precio1`.`precio_articulo` AS `precio_publico`,
    precio1.descripcion as descripcion1,
     `precio2`.`precio_articulo` AS `precio2`,
    precio2.descripcion as descripcion2,
	`precio3`.`precio_articulo` AS `precio3`,
    precio3.descripcion as descripcion3
FROM
    (((`articulo`
    JOIN `marcas` ON ((`marcas`.`codigo_marca` = `articulo`.`codigo_marca`)))
    JOIN `impuesto` ON ((`impuesto`.`codigo_impuesto` = `articulo`.`codigo_impuesto`)))
    JOIN (SELECT 
        `precios_articulos`.`codigo_articulo` AS `codigo_articulo`,
		`precios_articulos`.`precio_articulo` AS `precio_articulo`,
            precios.descripcion
    FROM
        `precios_articulos`
        inner join   precios  on(precios.codigo_precio=precios_articulos.codigo_precio)
    WHERE
        (`precios_articulos`.`codigo_precio` = 3)) precio3 ON ((`articulo`.`codigo_articulo` = `precio3`.`codigo_articulo`))
    JOIN (SELECT 
        `precios_articulos`.`codigo_articulo` AS `codigo_articulo`,
		`precios_articulos`.`precio_articulo` AS `precio_articulo`,
		`precios_articulos`.`codigo_precio` AS `precio1`,
            precios.descripcion
    FROM
        `precios_articulos`
        inner join   precios  on(precios.codigo_precio=precios_articulos.codigo_precio)
    WHERE
        (`precios_articulos`.`codigo_precio` = 2)) precio2 ON ((`articulo`.`codigo_articulo` = `precio2`.`codigo_articulo`))
    JOIN (SELECT 
        `precios_articulos`.`codigo_articulo` AS `codigo_articulo`,
		`precios_articulos`.`precio_articulo` AS `precio_articulo`,
		`precios_articulos`.`codigo_precio` AS `precio1`,
            precios.descripcion
    FROM
        `precios_articulos`
        inner join   precios  on(precios.codigo_precio=precios_articulos.codigo_precio)
    WHERE
        (`precios_articulos`.`codigo_precio` = 1)) precio1 ON ((`articulo`.`codigo_articulo` = `precio1`.`codigo_articulo`)))
         WHERE articulo.estado=1 and `precio1`.`precio_articulo`>0 
         order by `articulo`.`articulo` asc]]>
	</queryString>
