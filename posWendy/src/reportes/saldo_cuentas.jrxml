	<queryString>
		<![CDATA[SELECT cuentas_bancos.codigo, 
	cuentas_bancos.fecha, 
	cuentas_bancos.descripcion, 
	cuentas_bancos.referencia, 
	cuentas_bancos.debito, 
	cuentas_bancos.credito, 
	cuentas_bancos.saldo, 
	cuentas_bancos.codigo_banco, 
	bancos.id_tipo_cuenta,
	bancos.nombre, 
	bancos.no_cuenta
FROM cuentas_bancos INNER JOIN bancos ON cuentas_bancos.codigo_banco = bancos.id
WHERE 
	 DATE(cuentas_bancos.fecha) BETWEEN $P{fecha_min} AND $P{fecha_max} and cuentas_bancos.codigo_banco= $P{codigo_banco}]]>
